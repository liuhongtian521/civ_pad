<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">

    <LinearLayout
        android:layout_width="@dimen/dp_300"
        android:layout_height="@dimen/dp_254"
        android:background="@drawable/bg_write_round_five"
        android:orientation="vertical"
        android:visibility="visible">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_30">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_10"
                android:text="考生信息"
                android:textColor="#163366"
                android:textSize="@dimen/sp_10" />

            <ImageView
                android:id="@+id/iv_back"
                android:layout_width="@dimen/dp_20"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_gravity="right"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginRight="@dimen/dp_12"
                android:src="@drawable/cha" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#EFF2F7" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_10"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="@dimen/dp_145"
                android:layout_height="@dimen/dp_90"
                android:layout_gravity="center"
                android:background="@drawable/bg_photo_show"
                android:orientation="horizontal"
                android:padding="@dimen/dp_5">

                <ImageView
                    android:layout_width="@dimen/dp_65"
                    android:layout_height="@dimen/dp_80"
                    android:layout_alignParentLeft="true" />

                <ImageView
                    android:layout_width="@dimen/dp_65"
                    android:layout_height="@dimen/dp_80"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/dp_15">

                <TextView
                    android:id="@+id/tv_view_one"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_4"
                    android:text="姓名："
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_8" />

                <TextView
                    android:id="@+id/tv_student_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/tv_view_one"
                    android:layout_alignBottom="@+id/tv_view_one"
                    android:layout_toRightOf="@+id/tv_view_one"
                    android:text="李某某"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_8" />

                <TextView
                    android:id="@+id/tv_view_two"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_view_one"
                    android:layout_marginTop="@dimen/dp_12"
                    android:text="性别："
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_8" />

                <TextView
                    android:id="@+id/tv_student_sex"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/tv_view_two"
                    android:layout_alignBottom="@+id/tv_view_two"
                    android:layout_toRightOf="@+id/tv_view_two"
                    android:text="男"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_8" />

                <TextView
                    android:id="@+id/tv_view_three"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_view_two"
                    android:layout_marginTop="@dimen/dp_12"
                    android:text="民族："
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_8" />

                <TextView
                    android:id="@+id/tv_student_ethnic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/tv_view_three"
                    android:layout_alignBottom="@+id/tv_view_three"
                    android:layout_toRightOf="@+id/tv_view_three"
                    android:text="汉族"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_8" />

                <TextView
                    android:id="@+id/tv_view_four"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tv_view_three"
                    android:layout_marginTop="@dimen/dp_12"
                    android:text="验证结果："
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_8" />


                <ImageView
                    android:id="@+id/iv_student_type"
                    android:layout_width="@dimen/dp_34"
                    android:layout_height="@dimen/dp_12"
                    android:layout_alignTop="@+id/tv_view_four"
                    android:layout_alignBottom="@+id/tv_view_four"
                    android:layout_toRightOf="@+id/tv_view_four"
                    android:src="@drawable/icon_type_success" />
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15"
            android:background="@drawable/bg_write_line"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_25"
                android:layout_margin="@dimen/dp_1"
                android:background="#FAFAFA"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="@dimen/dp_122"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/dp_8">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="场次名称："
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />

                    <TextView
                        android:id="@+id/tv_exam_subjects"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="语文"
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="人脸对比："
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />

                    <TextView
                        android:id="@+id/tv_type_success"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="通过"
                        android:textColor="#0EBD35"
                        android:textSize="@dimen/sp_8" />

                    <TextView
                        android:id="@+id/tv_type_faile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="失败"
                        android:textColor="#FF8453"
                        android:textSize="@dimen/sp_8"
                        android:visibility="gone" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="（人照对比值："
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />

                    <TextView
                        android:id="@+id/tv_face_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="199.99"
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="）"
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="#FFE6E6E6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_25"
                android:layout_margin="@dimen/dp_1"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="@dimen/dp_122"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/dp_8">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="考场："
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />

                    <TextView
                        android:id="@+id/tv_examination_room"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="001"
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="准考证号："
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />

                    <TextView
                        android:id="@+id/tv_ticket_number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="21211020192922"
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />
                </LinearLayout>


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="#FFE6E6E6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_25"
                android:layout_margin="@dimen/dp_1"
                android:background="#FAFAFA"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="@dimen/dp_122"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:orientation="horizontal"
                    android:paddingLeft="@dimen/dp_8">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="座位号："
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />

                    <TextView
                        android:id="@+id/tv_seat_number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="36"
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center|left"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="身份证号："
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />

                    <TextView
                        android:id="@+id/tv_idCard"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="212110201929222112"
                        android:textColor="#666666"
                        android:textSize="@dimen/sp_8" />
                </LinearLayout>


            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="#FFE6E6E6" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_25"
                android:layout_margin="@dimen/dp_1"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/dp_8"
                android:gravity="center|left">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="户籍："
                    android:textColor="#666666"
                    android:textSize="@dimen/sp_8" />

                <TextView
                    android:id="@+id/tv_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="辽宁省沈阳市浑南新区世纪路1号"
                    android:textColor="#666666"
                    android:textSize="@dimen/sp_8" />

            </LinearLayout>

        </LinearLayout>


    </LinearLayout>


</LinearLayout>