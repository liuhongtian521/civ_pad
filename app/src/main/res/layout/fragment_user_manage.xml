<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/ll_user_manage_top"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="match_parent"
            android:gravity="center_vertical"
            android:layout_height="31dp">
            <TextView
                android:text="用户管理"
                android:textColor="#163366"
                android:textSize="9sp"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <View
            android:id="@+id/user_manage__line"
            app:layout_constraintTop_toBottomOf="@+id/ll_user_manage_top"
            android:background="@drawable/bg_driver"
            android:layout_width="match_parent"
            android:layout_height="1.5dp"/>
        <LinearLayout
            android:id="@+id/ll_user_manage__mode"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            app:layout_constraintTop_toBottomOf="@+id/user_manage__line">
            <RelativeLayout
                android:id="@+id/rl_user_manage__net"
                android:layout_width="match_parent"
                android:layout_height="31dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:text="用户名"
                    android:textColor="@color/_666666"
                    android:textSize="8sp" />
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:text="用户名"
                    android:textColor="#FF333333"
                    android:textAlignment="textEnd"
                    android:textSize="8sp" />
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1.5dp"
                    android:layout_alignParentBottom="true"
                    android:background="@drawable/bg_driver" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_user_manage_usb"
                android:layout_width="match_parent"
                android:layout_height="31dp">
                <TextView
                    android:text="旧密码"
                    android:layout_alignParentStart="true"
                    android:textSize="8sp"
                    android:layout_centerVertical="true"
                    android:textColor="@color/_666666"
                    android:layout_width="wrap_content"

                    android:layout_height="wrap_content"/>

                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:text=""
                    android:background="@null"
                    android:textColor="#FF333333"
                    android:textAlignment="textEnd"
                    android:hint="请输入"
                    android:textSize="8sp" />
                <View
                    android:background="@drawable/bg_driver"
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="1.5dp"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_user_manage_newpsd"
                android:layout_width="match_parent"
                android:layout_height="31dp">
                <TextView
                    android:text="新密码"
                    android:layout_alignParentStart="true"
                    android:textSize="8sp"
                    android:layout_centerVertical="true"
                    android:textColor="@color/_666666"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:text=""
                    android:background="@null"
                    android:textColor="#FF333333"
                    android:textAlignment="textEnd"
                    android:hint="请输入"
                    android:textSize="8sp" />

                <View
                    android:background="@drawable/bg_driver"
                    android:layout_alignParentBottom="true"
                    android:layout_width="match_parent"
                    android:layout_height="1.5dp"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_user_manage_sdcard"
                android:layout_width="match_parent"
                android:layout_height="31dp">
                <TextView
                    android:text="再次输入新密码"
                    android:layout_alignParentStart="true"
                    android:textSize="8sp"
                    android:layout_centerVertical="true"
                    android:textColor="@color/_666666"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <EditText
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:text=""
                    android:background="@null"
                    android:textColor="#FF333333"
                    android:textAlignment="textEnd"
                    android:hint="请输入"
                    android:textSize="8sp" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                >
            <TextView
                android:id="@+id/sb_time_set"
                android:layout_centerHorizontal="true"
                android:layout_width="390dp"
                android:layout_height="30dp"
                android:text="确认"
                android:background="@drawable/bg_network_btn"
                android:gravity="center"
                android:textSize="11sp"
                android:textColor="#FFFFFFFF"
                android:clickable="true"
                />
            </RelativeLayout>
        </LinearLayout>

        <!--        <TextView-->
        <!--            android:id="@+id/tv_lead"-->
        <!--            android:text="@string/data_lead_in"-->
        <!--            android:textColor="@color/white"-->
        <!--            android:gravity="center"-->
        <!--            android:textSize="11sp"-->
        <!--            android:layout_marginTop="128dp"-->
        <!--            app:layout_constraintTop_toBottomOf="@+id/ll_data_lead_mode"-->
        <!--            app:layout_constraintLeft_toLeftOf="parent"-->
        <!--            app:layout_constraintRight_toRightOf="parent"-->
        <!--            android:background="@drawable/bg_login_button"-->
        <!--            android:layout_width="390dp"-->
        <!--            android:layout_height="30dp"/>-->

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>